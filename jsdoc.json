{
    "source": {
        "include": ["src"],
        "includePattern": ".ts",
        "excludePattern": "(node_modules/|docs)"
    },
    "tags": {
        "allowUnknownTags": ["category"]
    },
    "plugins": [
        "node_modules/better-docs/typescript",
        "node_modules/better-docs/category"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/better-docs",
        "theme_opts": {
            "default_theme": "dark"
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}