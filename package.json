{
  "name": "@fauxjs/faux.js",
  "version": "0.1.0",
  "description": "Modern performant pseudo random data generator",
  "keywords": [
    "random",
    "pseudo",
    "fake",
    "data",
    "testing"
  ],
  "author": "Nicholas Mordecai",
  "url": "https://github.com/mecardo/faux.js",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./generators": "./dist/generators",
    "./locales": "./dist/locales",
    "./core": "./dist/core",
    "./api": "./dist/api"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "serve": "ts-node-dev src/api/server.ts",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.spec.ts\"",
    "test:watch": "mocha --require ts-node/register --recursive --watch --watch-files \"test/**/*.spec.ts\",\"src/**/*\" \"test/**/*.spec.ts\"",
    "test:coverage": "nyc npm run test",
    "lint": "eslint --ext .ts ./src/ --config eslint.js",
    "lint:fix": "eslint --ext .ts ./src/ --config eslint.js --fix",
    "circular-dep-check": "dpdm --exit-code circular:1 ./src/cli/index.ts"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.0",
    "md5": "^2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/benchmark": "^2.1.2",
    "@types/chai": "^4.3.3",
    "@types/glob": "^8.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.16",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "benchmark": "^2.1.4",
    "chai": "^4.3.6",
    "comment-parser": "^1.3.1",
    "cross-env": "^7.0.3",
    "dpdm": "^3.10.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.6.0",
    "fastify": "^4.11.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsup": "^6.5.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage",
    "lines": 0
  }
}
