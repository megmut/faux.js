{
  "name": "tseudo",
  "version": "0.0.1",
  "description": "Modern performant pseudo random data generator",
  "keywords": [
    "random",
    "pseudo",
    "fake",
    "data",
    "testing"
  ],
  "author": "Nicholas Mordecai",
  "url": "https://github.com/megmut/tseudo",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "preferGlobal": true,
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.spec.ts\"",
    "test:watch": "mocha --require ts-node/register --recursive --watch --watch-files \"test/**/*.spec.ts\",\"src/**/*\" \"test/**/*.spec.ts\"",
    "test:coverage": "nyc npm run test",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "performance": "ts-node scripts/run-performance",
    "build-docs": "jsdoc -c jsdoc.json",
    "circular-dep-check": "dpdm --exit-code circular:1 ./src/cli/index.ts"
  },
  "dependencies": {
    "typescript": "^4.9.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/benchmark": "^2.1.2",
    "@types/chai": "^4.3.3",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.16",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "benchmark": "^2.1.4",
    "better-docs": "^2.7.2",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dpdm": "^3.10.0",
    "eslint": "^8.23.0",
    "jsdoc": "^3.6.11",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "sinon": "^14.0.1",
    "ts-node": "^10.9.1"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage",
    "lines": 0
  }
}
